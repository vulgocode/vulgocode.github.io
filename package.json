{
  "name": "vulgocode",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Alexandre Ramos <alexandresantos84@gmail.com>",
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.1",
    "gatsby": "^2.9.11",
    "gatsby-image": "^2.1.4",
    "gatsby-plugin-google-analytics": "^2.1.0",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-offline": "^2.1.3",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-react-svg": "^2.1.1",
    "gatsby-plugin-sharp": "^2.1.9",
    "gatsby-plugin-styled-components": "^3.1.0",
    "gatsby-plugin-typography": "^2.3.0",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-source-filesystem": "^2.0.43",
    "gatsby-transformer-sharp": "^2.1.21",
    "nuka-carousel": "^4.5.8",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-media": "^1.9.2",
    "react-modal": "^3.8.1",
    "react-typography": "^0.16.19",
    "styled-components": "^4.3.2",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "gh-pages": "^2.0.1",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby clean && gatsby build",
    "develop": "gatsby develop",
    "serve": "gatsby serve -p 7000",
    "format": "prettier --write src/**/*.{js,jsx}",
    "lint": "eslint src/**/*.js",
    "lint:fix": "npm run lint -- --fix",
    "start": "npm run develop",
    "restart": "gatsby clean && npm run start",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "deploy": "npm run build --prefix-paths && gh-pages -d public --branch master"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vulgocode/vulgocode.github.io"
  },
  "bugs": {
    "url": "https://github.com/vulgocode/vulgocode.github.io/issues"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add"
    ]
  }
}
